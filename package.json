{
  "name": "how-to-madrid",
  "type": "module",
  "version": "0.0.1",
  "scripts": {
    "dev": "astro dev",
    "start": "astro dev",
    "build": "astro check && astro build",
    "build:analyze": "npm run build && npm run bundle-analyzer",
    "bundle-analyzer": "du -sh dist/ && find dist/ -name '*.js' -o -name '*.css' | head -10 | xargs ls -lh",
    "preview": "astro preview",
    "astro": "astro",
    "test": "vitest",
    "test:run": "vitest run",
    "biome:format": "npx @biomejs/biome format --write",
    "biome:lint": "npx @biomejs/biome lint",
    "biome:check": "npx @biomejs/biome check --write",
    "biome:check:unsafe": "npx @biomejs/biome check --write --unsafe",
    "biome:ci": "npx @biomejs/biome ci ."
  },
  "dependencies": {
    "@astrojs/check": "^0.9.5",
    "@astrojs/sitemap": "^3.6.0",
    "@astrojs/starlight": "^0.36.2",
    "astro": "^5.15.8",
    "astro-compress": "^2.3.8",
    "astro-og-canvas": "^0.7.2",
    "sharp": "^0.34.5",
    "starlight-links-validator": "^0.19.1",
    "typescript": "^5.9.3",
    "xml2js": "^0.6.2"
  },
  "devDependencies": {
    "@biomejs/biome": "2.3.5",
    "@vitest/ui": "^4.0.9",
    "vitest": "^4.0.3"
  }
}
